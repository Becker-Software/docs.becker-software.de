               xsi:noNamespaceSchemaLocation="https://resources.jetbrains.com/writerside/1.0/build-profiles.xsd">
    <sitemap priority="0.35" change-frequency="monthly"/>
    <variables>
        <offline-docs>true</offline-docs>
        <color-preset>frozen</color-preset>
        <showDownloadButton>true</showDownloadButton>
        <download-page>https://www.becker-software.de/</download-page>
        <download-title>Visit Website</download-title>
        <noindex-content>false</noindex-content>
        <custom-favicons>https://www.becker-software.de/favicon.ico</custom-favicons>
        <header-logo>https://www.becker-software.de/wp-content/uploads/2022/02/logo_2_white.png</header-logo>
        <product-web-url>https://docs.becker-software.de</product-web-url>
    </variables>
    <build-profile instance="hi"/>
    <footer>
        <copyright>Martin Becker Software</copyright>
        <link href="https://www.becker-software.de">Website</link>
        <link href="https://www.becker-software.de/blog/">Blog</link>
        <link href="https://portal.becker-software.de">Portal</link>
        <link href="https://www.becker-software.de/impressum/">Impressum</link>
        <link href="https://www.becker-software.de/datenschutzerklaerung/">Datenschutz</link>
    </footer>
